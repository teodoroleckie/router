{
  "name": "tleckie/router",
  "license": "MIT",
  "description": "Simple router PSR-7, PSR-17",
  "type": "library",
  "homepage": "https://github.com/teodoroleckie/router",
  "authors": [
    {
      "name": "Teodoro Leckie Westberg",
      "email": "teodoroleckie@gmail.com"
    }
  ],
  "keywords": [
    "tleckie",
    "routes",
    "router",
    "routing",
    "php",
    "psr-7",
    "psr-17",
    "psr-15"
  ],
  "autoload": {
    "psr-4": {
      "Tleckie\\Router\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tleckie\\Router\\Tests\\": "tests/"
    }
  },
  "provide": {
  },
  "require": {
    "php": "^8",
    "httpsoft/http-emitter": "^1.0",
    "httpsoft/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "infection/infection": "^0.21.5",
    "phpunit/phpunit": "^9.5",
    "mikey179/vfsstream": "^1.6",
    "friendsofphp/php-cs-fixer": "v3.0.0-beta.2"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "vendor-dir": "vendor",
    "bin-dir": "bin"
  },
  "scripts": {
    "fix-src": "./bin/php-cs-fixer fix src/ ",
    "fix-tests": "./bin/php-cs-fixer fix tests/",
    "tests": "./bin/phpunit ./tests/",
    "infection": "infection",
    "check": [
      "@fix-src",
      "@fix-tests",
      "@tests",
      "@infection"
    ]
  }
}
